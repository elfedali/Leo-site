models:
    User: { username: string, email: string, first_name: 'string nullable', last_name: 'string nullable', phone: 'string nullable', avatar: 'string nullable', status: 'string default:active', last_login_at: 'datetime nullable', email_verified_at: 'datetime nullable', password: string, remember_token: 'string:100 nullable' }
    Node: { uuid: 'string unique', owner_id: 'id foreign:users', title: string, slug: 'string unique', content: 'text  nullable', excerpt: 'text nullable', status: 'string default:draft', type: 'string default:shop', review_status: 'string default:open', phone: 'string nullable', relationships: { hasMany: 'NodeMeta,Term,Review,Attachment' } }
    NodeMeta: { node_id: 'id foreign:nodes', meta_key: string, meta_value: string }
    Term: { name: string, slug: string, taxonomy: 'string default:category', relationships: { hasMany: Node } }
    NodeTerm: { node_id: 'id foreign:nodes', term_id: 'id foreign:terms' }
    Attachment: { uuid: 'string unique', uploadedby_id: 'id foreign:users', title: 'string nullable', path: string, mime_type: string, size: 'integer default:0' }
    UserMeta: { user_id: 'id foreign:users', meta_key: string, meta_value: string }
    Review: { uuid: 'string unique', node_id: 'id foreign:nodes', reviewer_id: 'id foreign:users', rating: 'integer default:0', review: 'text nullable', status: 'string default:draft', parent_id: 'id foreign:reviews nullable' }
